<!DOCTYPE html>
<html>
<head>
  <title>CalfApp</title>
  <%= csrf_meta_tag %>
  <%= render 'layouts/stylesheets' %>
</head>
<body>

  <div class="container">
    <%= render 'layouts/header' %>
      <section class="round span-20">
        <%= yield %>
      </section>
    <%= render 'layouts/footer' %>
    <div class="span-20 last">
      <%= debug(params) if Rails.env.development? %>
    </div>
  </div>

</body>
</html>
